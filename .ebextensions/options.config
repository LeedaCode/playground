---
option_settings:
  aws:autoscaling:launchconfiguration:
    ImageId: ami-b0c2e6d5
    InstanceType: t2.micro
    IamInstanceProfile: pixi-playground-app-role
  aws:elasticbeanstalk:application:environment:
    NODE_ENV: production
    DB_TABLE_NAME: pixi-playground-items
    S3_BUCKET_NAME: pixi-playground-items
    DB_IS_PUBLIC_INDEX_NAME: isPublic-id-index
    DB_IS_FEATURED_INDEX_NAME: isFeatured-id-index
    DB_IS_OFFICIAL_INDEX_NAME: isOfficial-id-index
    APP_LOGS_GROUP_NAME: PIXI_Playground
    APP_LOGS_STREAM_NAME: App_Server
  aws:elasticbeanstalk:container:nodejs:
    NodeCommand: npm start
    ProxyServer: nginx
    GzipCompression: true
